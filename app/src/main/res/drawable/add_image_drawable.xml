<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="312dp"
                android:height="312dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/YP_Text_Gray"
                android:dashGap="32dp"
                android:dashWidth="32dp" />
            <corners android:radius="@dimen/search_corner_radius"/>
        </shape>
    </item>
    <item android:drawable="@drawable/add_image"
        android:width="80dp"
        android:height="80dp"
        android:gravity="center"/>
</layer-list>